
#define LED_GPIO 2 // GPIO2/TXD01
   //int br = ;
  int count = 0; 
void setup() {
  pinMode(LED_GPIO, OUTPUT);

}

void loop() { 
  while(count<=3)
  {  
    digitalWrite(LED_GPIO, LOW);
  delay(200);
  digitalWrite(LED_GPIO, HIGH);
    delay(2000);


  count ++;
  }
    //delay(2800);
    while (count>3&&count<=6){
    digitalWrite(LED_GPIO, HIGH);
    delay(2000);
    digitalWrite(LED_GPIO, LOW);
  delay(3000);
    count ++;
    }
    count=0;
   }
   
  
